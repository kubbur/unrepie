#!/home/kubbur/unrepie/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'unrepie','console_scripts','unrepie'
__requires__ = 'unrepie'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('unrepie', 'console_scripts', 'unrepie')()
    )
